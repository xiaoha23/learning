package com.xiaonan.unit3;

public class MethodReferenceExample1 {
	
	public static void main(String[] args) {
		
		//this lambda expression is a pass through, takes no argument and execute a method
		//this lambda expression only does method execution
		Thread t = new Thread(() -> printMessage());
		
		//Method reference
		Thread t1 = new Thread(MethodReferenceExample1::printMessage);
		t.start();
	}
	
	public static void printMessage() {
		System.out.println("Hello");
	}

}
