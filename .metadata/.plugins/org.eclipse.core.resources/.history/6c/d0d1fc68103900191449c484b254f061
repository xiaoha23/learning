package com.xiaonao;

public class RunnableExample {

	public static void main(String[] args) {
		
		Thread myThread = new Thread(new Runnable() {

			@Override
			public void run() {
				System.out.println("Printed inside Runnable");
			}
		});
		
		myThread.run();
		
		
		
		Thread myThreadLambda = new Thread(() -> System.out.println("Printed inside labmda Runnable"));
		myThreadLambda.run();
	}
}
